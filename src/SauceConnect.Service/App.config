<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="sauceLabsConfiguration" type="SauceConnect.Service.Configuration.SauceConnectSectionConfiguration, SauceConnect.Service" />
  </configSections>
  <sauceLabsConfiguration 
    Username="{USERNAME}" 
    AccessKey="{ACCESS KEY}"
    TunnelIdentifier="{IDENTIFIER}"
    TunnelPollInterval="60000"
    SauceConnectPath="\sc-4.3-win32\bin"
    />
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>